{
  "name": "@a-novel/connector-story-schematics",
  "private": false,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "lint:eslint": "pnpm dlx eslint .",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "format": "prettier --write . --config prettier.config.ts",
    "build": "rm -rf ./dist && vite build && tsc --build tsconfig.build.json",
    "test": "vitest",
    "publish:patch": "pnpm version patch && git push",
    "publish:minor": "pnpm version minor && git push",
    "publish:major": "pnpm version major && git push"
  },
  "license": "AGPL-3.0",
  "packageManager": "pnpm@10.12.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/connector-story-schematics.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    "./api": {
      "import": "./dist/api/api.es.js",
      "types": "./dist/api/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/hooks.es.js",
      "types": "./dist/hooks/index.d.ts"
    },
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=10.9.2",
    "node": ">=23.11.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.75.0",
    "react": "^19.1.0"
  },
  "dependencies": {
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/config-helpers": "^0.2.3",
    "@eslint/js": "^9.29.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "nock": "^14.0.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
